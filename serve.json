{
  "public": "out",
  "directoryListing": false,
  "cleanUrls": false,
  "trailingSlash": true,
  "headers": [
    {
      "source": "**/*.js",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/javascript"
        }
      ]
    },
    {
      "source": "**/*.css",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/css"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/**",
      "destination": "/index.html"
    }
  ]
}
